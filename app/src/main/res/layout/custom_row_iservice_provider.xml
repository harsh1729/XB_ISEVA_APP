<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingTop="3dp"
    android:paddingLeft="3dp"
    android:paddingRight="3dp"
    android:paddingBottom="1dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/selecter_home_category"
        android:orientation="horizontal"
        android:padding="1dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="vertical"
            android:layout_margin="3dp">

            <ImageView
                android:id="@+id/imgBrand"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal|center_vertical"
                android:scaleType="centerCrop"
                android:src="@drawable/default_user" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="3dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

            <TextView
                android:id="@+id/txtBrandName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:textStyle="bold"
                android:typeface="serif"
                android:maxLines="1"
                android:ellipsize="end"
                android:textColor="@drawable/selector_black_to_white_txt"
                android:text=""
                 />

            <TextView
                android:id="@+id/txtBrandQuality"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:typeface="serif"
                android:textColor="@android:color/black"
                android:text="" />

            <TextView
                android:id="@+id/txtBrandAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:typeface="serif"
                android:textColor="@android:color/black"
                android:text="" />

            </LinearLayout>
            <!--
              <TextView
               android:id="@+id/txtAddress"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textSize="@dimen/txt_app_labels_normal"
              android:maxLines="2"
              android:ellipsize="end"
              android:text=""
              android:textColor="@color/app_text"
              />
            -->


            <LinearLayout
                android:id="@+id/ratingLinear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="2dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"></LinearLayout>

                <Button

                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:text="Call"
                    android:id="@+id/buttonCall"
                    android:background="@drawable/selecter_call_btn_listview"
                     />


                <!--<LinearLayout
                    android:id="@+id/ratingLinearunder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selecter_home_category"
                    android:clickable="true"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="2dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@android:drawable/btn_star_big_on" />

                    <TextView
                        android:id="@+id/rate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text=""
                        android:textColor="@drawable/selector_text_black_to_white" />
                </LinearLayout>-->

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

    <!--<LinearLayout
        android:id="@+id/linearratingView"
